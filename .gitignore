# === TLS Artifact Robot .gitignore ===
# Ignore OS-specific junk
.DS_Store
Thumbs.db

# Ignore Python cache and build artifacts
__pycache__/
*.pyc
*.pyo
*.pyd
*.egg-info/
build/
dist/

# Ignore virtual environments
venv/
.env/
.venv/

# Ignore logs and temp files
*.log
*.tmp
*.swp

# Ignore TLS/SSL artifacts (generated, not source-controlled)
*.crt
*.csr
*.key
*.pem
*.p12
*.der

# Ignore secrets and environment configs
.env.local
.env.secret
secrets.json

# Ignore container/runtime artifacts
*.pid
*.sock
*.db
*.sqlite

# Ignore IDE/editor configs
.vscode/
.idea/
*.iml

# Ignore coverage and test reports
.coverage
htmlcov/
*.cov
*.lcov

